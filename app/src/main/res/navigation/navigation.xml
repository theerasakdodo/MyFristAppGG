<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/aboutmeFragment">
    <fragment
        android:id="@+id/aboutmeFragment"
        android:name="com.example.myfristapp.AboutmeFragment"
        android:label="fragment_aboutme"
        tools:layout="@layout/fragment_aboutme" >
        <action
            android:id="@+id/action_aboutmeFragment_to_contectFragment"
            app:destination="@id/contectFragment" />
    </fragment>
    <fragment
        android:id="@+id/contectFragment"
        android:name="com.example.myfristapp.ContectFragment"
        android:label="fragment_contect"
        tools:layout="@layout/fragment_contect" >
        <action
            android:id="@+id/action_contectFragment_to_aboutmeFragment"
            app:destination="@id/aboutmeFragment" />
    </fragment>
</navigation>